<link rel="stylesheet" href="/resources/css/stylesheet.css?v=<%= locals.env.version %>">
<link rel="canonical" href="<%= locals.server.onlyUrl %>">

<script defer src="/resources/js/lib/velocity.min.js?v=2.0.6" nonce="<%= locals.server.nonce %>" crossorigin="anonymous"></script>
<script defer src="/resources/js/utils.js?v=<%= locals.env.version %>" nonce="<%= locals.server.nonce %>"></script>
<script defer src="/resources/js/main.js?v=<%= locals.env.version %>" nonce="<%= locals.server.nonce %>"></script>
