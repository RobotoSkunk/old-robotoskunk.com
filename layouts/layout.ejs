<% if (locals.html.head) { %><%- locals.html.head.replace(/\t/g, '') %><% }%>


<div class="nav-bg mobile" data-action="close" data-type="menu-toggle" style="display: none"></div>

<header class="no-select">
	<button class="menu-btn mobile" data-action="open" data-type="menu-toggle" aria-label="Open navigation menu">
		<svg viewBox="0 0 50 34" fill="none">
			<rect width="50" height="8" rx="4"/>
			<rect y="13" width="50" height="8" rx="4"/>
			<rect y="26" width="50" height="8" rx="4"/>
		</svg>
	</button>

	<nav>
		<button class="menu-btn black mobile" data-action="close" data-type="menu-toggle" style="width: 100%" aria-label="Close navigation menu">
			<svg viewBox="0 0 50 34" fill="none">
				<rect width="50" height="8" rx="4"/>
				<rect y="13" width="50" height="8" rx="4"/>
				<rect y="26" width="50" height="8" rx="4"/>
			</svg>
		</button>
		<div>
			<% if (locals.user) { %>
				<div class="dropdown">
					<button id="header-display-name" class="btn" style="min-width: 150px;"><span style="max-width: 100%" class="txt-ellipsis"><%= locals.user.name %></span></button>
					<div class="options">
						<a href="<%= locals.user.url %>" id="myacc-path">Profile</a>
						<a href="/accounts/settings">Settings</a>
						<a href="/accounts/logout" rel="nofollow">Logout</a>
					</div>
				</div>
			<% } else { %>
				<a class="login-btn" href="/accounts/signin" rel="noreferer">Login</a>
				<a class="login-btn alt" href="/accounts/signup" rel="noreferer">Sign up</a>
			<% } %>
		</div>
		<div>
			<a href="/commissions" class="btn">Commissions</a>
			<a href="/store" class="btn">Online store</a>
			<div class="dropdown" style="min-width: 170px;">
				<button class="btn" style="min-width: 170px;">Games</button>
				<div class="options">
					<a href="https://gamejolt.com/games/pixelman/410001">PixelMan Adventures (beta)</a>
					<a href="https://www.newgrounds.com/portal/view/807131">E-GG (demo)</a>
				</div>
			</div>
		</div>
	</nav>

	<a href="/" title="Go to home page" class="logo">
		<img width="100px" height="100px" src="/resources/svg/logo_2020.svg" alt="RobotoSkunk's logo">
	</a>
</header>

